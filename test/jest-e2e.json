{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "@lib/typeorm/(.*)": "<rootDir>/../libs/typeorm/src/$1",
    "@lib/typeorm": "<rootDir>/../libs/typeorm/src",
    "@lib/entities/(.*)": "<rootDir>/../libs/entities/src/$1",
    "@lib/entities": "<rootDir>/../libs/entities/src",
    "@lib/comment/(.*)": "<rootDir>/../libs/comment/src/$1",
    "@lib/comment": "<rootDir>/../libs/comment/src",
    "@lib/providers/(.*)": "<rootDir>/../libs/providers/src/$1",
    "@lib/providers": "<rootDir>/../libs/providers/src",
    "@lib/user/(.*)": "<rootDir>/../libs/user/src/$1",
    "@lib/user": "<rootDir>/../libs/user/src",
    "@lib/auth/(.*)": "<rootDir>/../libs/auth/src/$1",
    "@lib/auth": "<rootDir>/../libs/auth/src",
    "@lib/shared/(.*)": "<rootDir>/../libs/shared/src/$1",
    "@lib/shared": "<rootDir>/../libs/shared/src",
    "@lib/files/(.*)": "<rootDir>/../libs/files/src/$1",
    "@lib/files": "<rootDir>/../libs/files/src"
  }
}