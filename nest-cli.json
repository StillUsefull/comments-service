{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true
  },
  "projects": {
    "typeorm": {
      "type": "library",
      "root": "libs/typeorm",
      "entryFile": "index",
      "sourceRoot": "libs/typeorm/src",
      "compilerOptions": {
        "tsConfigPath": "libs/typeorm/tsconfig.lib.json"
      }
    },
    "entities": {
      "type": "library",
      "root": "libs/entities",
      "entryFile": "index",
      "sourceRoot": "libs/entities/src",
      "compilerOptions": {
        "tsConfigPath": "libs/entities/tsconfig.lib.json"
      }
    },
    "comment": {
      "type": "library",
      "root": "libs/comment",
      "entryFile": "index",
      "sourceRoot": "libs/comment/src",
      "compilerOptions": {
        "tsConfigPath": "libs/comment/tsconfig.lib.json"
      }
    },
    "providers": {
      "type": "library",
      "root": "libs/providers",
      "entryFile": "index",
      "sourceRoot": "libs/providers/src",
      "compilerOptions": {
        "tsConfigPath": "libs/providers/tsconfig.lib.json"
      }
    },
    "user": {
      "type": "library",
      "root": "libs/user",
      "entryFile": "index",
      "sourceRoot": "libs/user/src",
      "compilerOptions": {
        "tsConfigPath": "libs/user/tsconfig.lib.json"
      }
    },
    "auth": {
      "type": "library",
      "root": "libs/auth",
      "entryFile": "index",
      "sourceRoot": "libs/auth/src",
      "compilerOptions": {
        "tsConfigPath": "libs/auth/tsconfig.lib.json"
      }
    },
    "shared": {
      "type": "library",
      "root": "libs/shared",
      "entryFile": "index",
      "sourceRoot": "libs/shared/src",
      "compilerOptions": {
        "tsConfigPath": "libs/shared/tsconfig.lib.json"
      }
    },
    "files": {
      "type": "library",
      "root": "libs/files",
      "entryFile": "index",
      "sourceRoot": "libs/files/src",
      "compilerOptions": {
        "tsConfigPath": "libs/files/tsconfig.lib.json"
      }
    }
  }
}